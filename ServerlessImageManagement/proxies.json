{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "Proxy for index": {
      "matchCondition": {
        "route": "/"
      },
      "backendUri": "https://myphotostorage2018.blob.core.windows.net/frontend/index.html"
    },
    "Proxy for paths": {
      "matchCondition": {
        "route": "/app/{*path}"
      },
      "backendUri": "https://myphotostorage2018.blob.core.windows.net/frontend/{path}"
    }
  }
}
